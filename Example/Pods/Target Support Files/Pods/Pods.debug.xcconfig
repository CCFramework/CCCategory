GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CCCategory" "${PODS_ROOT}/Headers/Public/GTMBase64"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CCCategory" -isystem "${PODS_ROOT}/Headers/Public/GTMBase64"
OTHER_LDFLAGS = $(inherited) -ObjC -l"CCCategory" -l"GTMBase64" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods